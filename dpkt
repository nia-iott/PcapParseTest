import dpkt
import time
import socket

start = time.perf_counter()


def print_pcap(pcap):
    for (ts, buf) in pcap:
        try:
            eth = dpkt.ethernet.Ethernet(buf)
            ip = eth.data
            # read the source IP in src
            src = socket.inet_ntoa(ip.src)
            # read the destination IP in dst
            dst = socket.inet_ntoa(ip.dst)

            # Print the source and destination IP
            print('Source: ' + src + ' Destination: ' + dst)
        except:
            pass


def main():
    f = open('wrccdc.2018-03-23.010014000000000.pcap', 'rb')
    pcap = dpkt.pcap.Reader(f)
    print_pcap(pcap)


if __name__ == '__main__':
    main()

finish = time.perf_counter()
print (f'This took {round(finish-start, 2)} seconds')
